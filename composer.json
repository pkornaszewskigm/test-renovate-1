    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-xml": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "paragonie/random_compat": "*",
        "ralouphie/getallheaders": "*"
    },
    "prefer-stable": true,
    "minimum-stability": "beta",
    "scripts": {
        "prepare-test-env": [
            "bin/console cache:clear --no-warmup --no-ansi --no-interaction --no-debug --env=test",
            "bin/console cache:warmup --no-ansi --no-interaction --no-debug --env=test",
            "bin/console cache:pool:clear cache.app_clearer --env=test --no-interaction",
            "bin/console doctrine:database:create --if-not-exists --no-ansi --no-interaction --no-debug --env=test",
            "bin/console doctrine:schema:update --force --no-ansi --no-interaction --no-debug --env=test"
        ],
        "after-test-env": [
            "bin/console doctrine:schema:drop --force --no-ansi --no-interaction --no-debug --env=test"
        ],

        "after-build": [
            "bin/console musement:deploy:helper --alive-file-action=remove --no-ansi --no-interaction --no-debug --env=prod",
            "bin/console assets:install web --no-ansi --no-interaction --no-debug --env=prod",
            "bin/console ckeditor:install --exclude=samples --release=standard --tag=4.16.2 --clear=drop --no-progress-bar --no-ansi --no-interaction --env=prod",
            "bin/console cache:clear --no-warmup --no-ansi --no-interaction --no-debug --env=dev",
            "bin/console cache:clear --no-warmup --no-ansi --no-interaction --no-debug --env=test",
            "bin/console cache:clear --no-warmup --no-ansi --no-interaction --no-debug --env=prod"
        ],
        "after-deploy": [
            "bin/console cache:warmup --no-ansi --no-interaction --no-debug --env=prod",
            "bin/console musement:deploy:helper --alive-file-action=create --no-ansi --no-interaction --no-debug --env=prod"
        ],

        "msm-check-sec": [
            "local-php-security-checker --update-cache",
            "local-php-security-checker"
        ],
        "msm-check-cs": [
            "bin/msm-cs-fix.sh --dry-run"
        ],
        "msm-check": [
            "@msm-check-sec",
            "@msm-check-cs"
        ],
        "msm-export-routes": [
            "bin/console fos:js-routing:dump --env=prod --no-debug --ansi --no-interaction"
        ],

        "msm-ckeditor": "Musement\\CommonBundle\\Composer\\CustomScripts::installCKEditorAssets",
        "msm-install-git-hooks": "Musement\\CommonBundle\\Composer\\CustomScripts::installGitHooks",
        "symfony-scripts": [
            "bin/console cache:clear --no-warmup --no-interaction --no-debug",
            "bin/console cache:warmup --no-interaction --no-debug",
            "bin/console ckeditor:install --exclude=samples --release=standard --tag=4.16.2 --clear=drop --no-progress-bar --no-ansi --no-interaction --env=prod",
            "bin/console assets:install web --no-interaction --no-debug"
        ],
        "post-update-cmd": [
            "@msm-install-git-hooks",
            "@symfony-scripts"
        ],
        "post-install-cmd": [
            "@msm-install-git-hooks",
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "composer msm-check-sec": "script",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "require": "^4.4"
        },
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "dist-file": "app/config/parameters.yml.dist",
            "keep-outdated": false
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/musement/LiformBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/musement/Liform"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mbeccati/wsdl2phpgenerator"
        }
    ],
    "autoload": {
        "psr-4": {
            "Musement\\": "src/Musement",
            "Application\\Sonata\\": "src/Application/Sonata/",
            "QualityAssurance\\": "src/QualityAssurance/",
            "Omnipay\\AdyenCSE\\": "src/Omnipay/AdyenCSE/"
        },
        "exclude-from-classmap": [
            "src/Musement/*/*/Test/",
            "src/Musement/*/*/Tests/",
            "src/Musement/*/*/*/Tests/"
        ],
        "classmap": [
            "src/Application.php",
            "src/Kernel.php"
        ],
        "files": ["config/autoload.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "QualityAssurance\\": "vendor/musement/core-fixtures/src"
        },
        "files": ["vendor/symfony/var-dumper/Resources/functions/dump.php"]
    },
    "config": {
        "process-timeout": 3600,
        "preferred-install": {
            "musement/*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true
        }
    }
}
